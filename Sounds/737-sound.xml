     <equals>
        <property>sim/current-view/view-number</property>
        <value>0</value>
     </equals>
    <less-than>
        <property>engines/engine[0]/n1</property>
        <value>30</value>
    </less-than>
   <and>
    <property>controls/engines/engine[0]/starter</property>     
   </and>
   </condition>
  <volume>
   <property>sim/sound/Pvolume</property>
  </volume>
<pitch>
<property>/engines/engine[0]/n1</property>
<offset>0.10</offset>
<factor>0.01</factor>
</pitch>
</engine>

<engine>
   <name>Right engine sound starter inside</name>
   <path>Sounds/jet.wav</path>
   <mode>looped</mode>
   <condition>
     <equals>
        <property>sim/current-view/view-number</property>
        <value>0</value>
     </equals>
    <less-than>
        <property>engines/engine[1]/n1</property>
