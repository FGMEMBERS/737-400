<?xml version="1.0"?>
 
<PropertyList>

 <fx>

<engine>
 <name>Left engine off</name>
 <path>Sounds/turbine_shutdown.wav</path>
 <mode>once</mode>
 <condition>
 <and>
  <property>controls/engines/engine[0]/cutoff</property>
 </and>
 </condition>
  <volume>
   <property>sim/sound/Evolume</property>
  </volume>
  <position>
      <x>3.69645</x>
      <y>4.77041</y>
      <z>-1.30186</z>
 </position>
 <reference-dist>10.0</reference-dist>
 <max-dist>50.0</max-dist>
 </engine>

