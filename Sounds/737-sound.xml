 <engine>
   <name>Right engine sound starter outside</name>
   <path>Sounds/whine.wav</path>
   <mode>looped</mode>
   <condition>
      <greater-than>
        <property>sim/current-view/view-number</property>
        <value>0</value>
      </greater-than>
       <less-than>
        <property>engines/engine[1]/n1</property>
        <value>30</value>
