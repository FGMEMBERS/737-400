   <volume>
    <property>sim/sound/Ovolume</property>
   </volume>
<pitch>
<property>/engines/engine[1]/n1</property>
<offset>0.20</offset>
<factor>0.05</factor>
</pitch>
<position>
      <x>3.69645</x>
      <y>-4.77041</y>
      <z>-1.30186</z>
</position>
<reference-dist>200.0</reference-dist>
<max-dist>500.0</max-dist>
 </engine>

  <engine>
   <name>Left engine sound inside</name>
   <path>realt154/crj7_bn22.wav</path>
   <mode>looped</mode>
   <condition>
   <and>
      <not>
        <property>controls/engines/engine[0]/cutoff</property>
      </not>
      <equals>
        <property>sim/current-view/view-number</property>
        <value>0</value>
      </equals>
    <property>/engines/engine[0]/n1</property>      
   </and>
   </condition>
   <volume>
    <property>/engines/engine[0]/thrust_lb</property>
    <type>ln</type>
    <factor>0.05</factor>
    <offset>0.05</offset>
   </volume>
   <pitch>
    <property>/engines/engine[0]/thrust_lb</property>
    <type>log</type>
    <factor>0.26</factor>
    <offset>0.1</offset>
</pitch>
  </engine>

  <engine>
   <name>Right engine sound inside</name>
   <path>realt154/crj7_bn22.wav</path>
   <mode>looped</mode>
   <condition>
