<switch>
   <name>click on</name>
   <mode>once</mode>
   <path>Aircraft/737-400/Sounds/click.wav</path>
   <condition>
<property>/sim/sound/switch</property>
</condition>
   <volume>
    <factor>0.45</factor>
   </volume>
  </switch>

<switch>
   <name>click off</name>
   <mode>once</mode>
   <path>Aircraft/737-400/Sounds/click.wav</path>
   <condition>
   <not>
<property>/sim/sound/switch</property>
</not>
</condition>
   <volume>
    <factor>0.45</factor>
   </volume>
  </switch>
  
  <switch>
   <name>Autopilot Disengage</name>
   <mode>once</mode>
   <path>Aircraft/737-400/Sounds/apdisengage.wav</path>
   <condition>
   <and>
   <equals>
    <property>/autopilot/locks/cmda</property>
	<value>0</value>
	</equals>
	<equals>
	<property>/autopilot/locks/cmda-disengage</property>
	<value>1</value>
	</equals>
   </and>
</condition>
   <volume>
    <factor>0.45</factor>
   </volume>
  </switch>

     <rain>
	    <name>rain</name>
	    <mode>looped</mode>
	    <path>Aircraft/737-400/Sounds/rain.wav</path>
	    <condition>
		    <and>
			    <equals>
				    <property>environment/rain/above_clouds</property>
				    <value>0</value>
			    </equals>
			    <or>
				    <greater-than>
					    <property>environment/metar/rain-norm</property>
					    <value>0.2</value>
				    </greater-than>
				    <greater-than>
					    <property>environment/metar/snow-norm</property>
					    <value>0.2</value>
				    </greater-than>
			    </or>
		    </and>
	 </condition>
	    <volume>
		    <property>/environment/metar/rain-norm</property>
		    <factor>1.00</factor>
		    <offset>0.00</offset>
		    <min>0.4</min>
		<max>0.9</max>
	    </volume>
	    <pitch>
		    <property>/velocities/airspeed-kt</property>
		    <factor>0.001</factor>
		    <offset>1.0</offset>
            </pitch>
      </rain>

