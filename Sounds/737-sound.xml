    <factor>0.05</factor>
    <offset>0.05</offset>
   </volume>   
   <pitch>
    <property>/engines/engine[1]/thrust_lb</property>
    <type>log</type>
    <factor>0.26</factor>
    <offset>0.1</offset>
</pitch>
  </engine>

