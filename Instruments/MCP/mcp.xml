<?xml version="1.0"?>
<PropertyList>

 <path>mcp.ac</path>
 
 <animation>
        <type>material</type>
            <object-name>base</object-name>
     		<object-name>disk</object-name>
		<object-name>n1_box</object-name>
		<object-name>speed_box</object-name>
		<object-name>cylinder</object-name>
		<object-name>lvl_box</object-name>
