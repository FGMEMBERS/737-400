		<object-name>alt_cylinder</object-name>
		<object-name>vs_box</object-name>
		<object-name>vs_rect</object-name>
		<object-name>box1</object-name>
		<object-name>box2</object-name>
		<object-name>box3</object-name>
		<object-name>box4</object-name>
		<object-name>rect1</object-name>
		<object-name>crz_disk2</object-name>
		<object-name>crz_cylinder2</object-name>
		<object-name>crz_disk1</object-name>
		<object-name>crz_cylinder1</object-name>
            <object-name>box5</object-name>
            <object-name>at_disk1</object-name>
            <object-name>at_disk2</object-name>
		<object-name>vnav</object-name>
		<object-name>lnav</object-name>
		<object-name>app</object-name>
		<object-name>vor_loc</object-name>
		<object-name>hdg_sel</object-name>
		<object-name>alt_hld</object-name>
		<object-name>v/s</object-name>
		<object-name>cmda</object-name>
		<object-name>speed</object-name>
      <emission>
	   <red>0.3</red>
	   <green>0.3</green>
	   <blue>0.3</blue>
            <factor-prop>controls/lighting/panel-norm</factor-prop>	
      </emission>
        </animation>

    <animation>
        <type>material</type>
        <object-name>MCP</object-name>
        <emission>
    <red>0.3</red>
    <green>0.3</green>
    <blue>0.3</blue>
            <factor-prop>controls/lighting/panel-norm</factor-prop>
        </emission>
    </animation>

 <animation>
  <type>select</type>
  <object-name>cmda</object-name>
    <condition>
    <equals>
     <property>/autopilot/locks/cmda</property>
      <value>1</value> 
    </equals>
    </condition>
 </animation>
 
  <animation>
  <type>select</type>
  <object-name>hdg_sel</object-name>
  <condition>
   <equals>
    <property>/autopilot/locks/heading</property>
    <value>dg-heading-hold</value>
   </equals>
  </condition>
 </animation>
 
 <animation>
 <type>select</type>
 <object-name>aton</object-name>
 <object-name>atlight</object-name>
  <condition>
  <equals>
  <property>/autopilot/locks/at</property>
  <value>1</value>
  </equals>
  </condition>
  </animation>
 
  <animation>
 <type>select</type>
 <object-name>atoff</object-name>
  <condition>
  <equals>
  <property>/autopilot/locks/at</property>
  <value>0</value>
  </equals>
  </condition>
  </animation>
 
<animation>
  <type>select</type>
  <object-name>speed</object-name>
  <condition>
  <or>
  	<and>
     	  <equals>
    	  	<property>/autopilot/locks/speed</property>
    		<value>speed-with-throttle</value>
    	 </equals>
    	 <equals>
    		<property>/autopilot/locks/vnav</property>
    		<value>0</value>
    	 </equals>
   	</and>
  	<and>
     	  <equals>
    	  	<property>/autopilot/locks/speed</property>
    		<value>mach-hold</value>
    	 </equals>
    	 <equals>
    		<property>/autopilot/locks/vnav</property>
    		<value>0</value>
    	 </equals>
   	</and>
  </or>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  	<object-name>vnav</object-name>
  	<condition>
		<or>
  			<and>
   				<equals>
    					<property>/autopilot/locks/altitude</property>
    					<value>altitude-hold</value>
   				</equals>
   				<equals>
  				 	<property>/autopilot/locks/speed</property>
 	 <value>speed-with-throttle</value>
  	</equals>
	<equals>
   <property>autopilot/locks/vnav</property>
   <value>1</value>
   </equals>
  	</and>
	 <and>
    <equals>
    <property>/autopilot/locks/altitude</property>
    <value>altitude-hold</value>
    </equals>
    <equals>
    <property>/autopilot/locks/speed</property>
 	<value>mach-hold</value>
    </equals>
	<equals>
   <property>autopilot/locks/vnav</property>
   <value>1</value>
   </equals>
   	</and>
		</or>
  	</condition>
 </animation>

  <animation>
  <type>select</type>
  <object-name>alt_hld</object-name>
  <condition>
  <and>
   <equals>
    <property>/autopilot/locks/altitude</property>
    <value>vertical-speed-hold</value>
   </equals>
     <equals>
    <property>/autopilot/settings/vertical-speed-fpm</property>
    <value>0</value>
   </equals>
   </and>
  </condition>
 </animation>
 
  <animation>
  <type>select</type>
  <object-name>app</object-name>
  <condition>
   <and>
   <equals>
     <property>/autopilot/locks/heading</property>
     <value>nav1-hold</value>
   </equals>
   <equals>
     <property>/autopilot/locks/altitude</property>
     <value>gs1-hold</value>
   </equals>
  </and>
  </condition>
 </animation>
 
  <animation>
  <type>select</type>
  <object-name>vor_loc</object-name>
  <condition>
  <and>
    <property>/instrumentation/nav[0]/nav-loc</property>
    <property>/autopilot/locks/nav[0]</property>
  </and>
  </condition>
 </animation>
 
  <animation>
  <type>select</type>
  <object-name>lnav</object-name>
  <condition>
   <equals>
   <property>/autopilot/locks/heading</property>
    <value>true-heading-hold</value>
   </equals>
  </condition>
 </animation>

 <animation>
  <type>select</type>
  <object-name>apspd1</object-name>
  <object-name>apspd2</object-name>
  <object-name>apspd3</object-name>
  <condition>
   <and>
   <equals>
   <property>/autopilot/locks/vnav</property>
    <value>0</value>
   </equals>
   <equals>
   <property>/instrumentation/flightdirector/mach-mode</property>
    <value>0</value>
   </equals>
   </and>
  </condition>
 </animation>
 
  <animation>
  <type>select</type>
  <object-name>machp</object-name>
  <object-name>mach1</object-name>
  <object-name>mach2</object-name>
  <condition>
  <and>
     <equals>
   <property>/autopilot/locks/vnav</property>
    <value>0</value>
   </equals>
  <equals>
   <property>/instrumentation/flightdirector/mach-mode</property>
    <value>1</value>
  </equals>
  </and>
  </condition>
 </animation>
 
 <!-- AutoPilot Speed -->
 
 <animation>
  <type>textranslate</type>
  <object-name>mach1</object-name>
  <property>/autopilot/settings/target-speed-mach</property>
  <factor>1.0</factor>
  <step>0.1</step>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 
<animation>
  <type>textranslate</type>
  <object-name>mach2</object-name>
  <property>/autopilot/settings/target-speed-mach</property>
