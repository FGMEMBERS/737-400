<?xml version="1.0"?>
<PropertyList>

 <path>oiltemp.ac</path>

 <animation>
  <type>material</type>
  <object-name>Face</object-name>
  <object-name>Needle1</object-name>
<emission>
	 <red>0.3</red>
	 <green>0.25</green>
	 <blue>0.2</blue>
	 <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>	
 </emission>
   </animation>

 <animation>
  <type>rotate</type>
  <object-name>Needle1</object-name>
  <property>/engines/engine[0]/oil-temperature-degf</property>
  <interpolation>
   <entry>
    <ind>0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>100.0</ind>
    <dep>270</dep>
   </entry>
   <entry>
    <ind>300.0</ind>
    <dep>360</dep>
   </entry>
  </interpolation>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>


