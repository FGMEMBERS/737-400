  <equals>
  <property>/surface-positions/flap-pos-norm</property>
  <value>0.125</value>
  </equals>
    <equals>
  <property>/surface-positions/flap-pos-norm</property>
  <value>0.250</value>
  </equals>
   <equals>
  <property>/surface-positions/flap-pos-norm</property>
  <value>0.375</value>
  </equals>
    <equals>
  <property>/surface-positions/flap-pos-norm</property>
  <value>0.500</value>
  </equals>
    <equals>
  <property>/surface-positions/flap-pos-norm</property>
  <value>0.625</value>
  </equals>
    <equals>
  <property>/surface-positions/flap-pos-norm</property>
  <value>0.750</value>
  </equals>
